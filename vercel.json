{
    "version": 2,
    "functions": {
	   "api/*.php": {
	      "runtime": "vercel-php@0.6.0"
	    }
	},
    "routes": [
        {
            "src": "/(css|js|static|extend|min)/(.*)",
            "dest": "public/$1/$2",
            "has":[
                {"type": "query", "key": "v"}
            ]
        },
        {
            "src": "/(css|js|static|extend|min)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Vercel oneblog 2",
        "APP_ENV": "production",
        "APP_URL": "https://oneblog2.vercel.app",
        "VERCEL_DEMO_MODE": "true",
        "NODE_PATH": "node"
    }
}
